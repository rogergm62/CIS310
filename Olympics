CREATE TABLE olympic_Data (
    RowID INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
    ID INT,
    AthName VARCHAR(25),
    Gender CHAR(1),
    Age INT,
    Height INT,
    Weight INT,
    Team VARCHAR(25),
    NOC CHAR(3),
    Games VARCHAR(50),
    OlympicYear INT,
    Season VARCHAR(10),
    City VARCHAR(20),
    Sport VARCHAR(100),
    OlympicEvent VARCHAR(100),
    Medal VARCHAR(10)
);
